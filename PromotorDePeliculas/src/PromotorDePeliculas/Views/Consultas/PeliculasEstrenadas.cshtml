@model IEnumerable<PromotorDePeliculas.Models.ModelsVista.PeliculasEstren>

@{
    ViewBag.Title = "PeliculasEstrenadas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h1>Peliculas estrenadas</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Titulo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaEstreno)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Titulo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaEstreno)
                </td>
            </tr>
        }
    <tbody>
</table>

